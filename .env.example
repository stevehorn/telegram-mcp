# Telegram API Credentials
# Get these from https://my.telegram.org
TELEGRAM_API_ID=your_api_id_here
TELEGRAM_API_HASH=your_api_hash_here

# Your phone number (with country code, e.g., +1234567890)
TELEGRAM_PHONE=+1234567890

# Session string (will be generated after first authentication)
TELEGRAM_SESSION=

# Group ID or invite hash (from the invite link)
# For a link like https://t.me/+ABC123XYZ, use the hash: ABC123XYZ
# For a public group like https://t.me/mygroup, use: mygroup
TELEGRAM_GROUP_ID=your_group_id_or_invite_hash
